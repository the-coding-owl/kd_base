{
    "name": "the-coding-owl/kd-base",
    "description": "A base distribution that provides a basic configuration for TYPO3 CMS",
    "type": "typo3-cms-extension",
    "require": {
        "typo3/minimal": "^10.1.0 || ^11.5.0",
        "typo3/cms-fluid-styled-content": "^10.1.0 || ^11.5.0",
        "typo3/cms-belog": "^10.1.0 || ^11.5.0",
        "typo3/cms-beuser": "^10.1.0 || ^11.5.0",
        "typo3/cms-info": "^10.1.0 || ^11.5.0",
        "typo3/cms-linkvalidator": "^10.1.0 || ^11.5.0",
        "typo3/cms-lowlevel": "^10.1.0 || ^11.5.0",
        "typo3/cms-rte-ckeditor": "^10.1.0 || ^11.5.0",
        "typo3/cms-reports": "^10.1.0 || ^11.5.0",
        "typo3/cms-form": "^10.1.0 || ^11.5.0",
        "typo3/cms-setup": "^10.1.0 || ^11.5.0",
        "typo3/cms-tstemplate": "^10.1.0 || ^11.5.0",
        "typo3/cms-viewpage": "^10.1.0 || ^11.5.0",
        "the-coding-owl/easy-navigation": "^2.1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Kevin Ditscheid",
            "email": "kevin@the-coding-owl.de",
            "role": "Owner"
        }
    ],
    "support": {
        "issues": "https://github.com/the-coding-owl/kd_base/issues",
        "source": "https://github.com/the-coding-owl/kd_base"
    },
    "homepage": "https://github.com/the-coding-owl/kd_base",
    "extra": {
        "typo3/cms": {
            "extension-key": "kd_base"
        }
    }
}
